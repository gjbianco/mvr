{
  "manifest_version": 2,

  "name": "mvr",
  "description": "minimum viable reddit",
  "version": "1.1.0",

  "icons": {
    "128": "assets/mvr1.png"
  },

  "content_scripts": [
    {
      "matches": ["https://www.reddit.com/*"],
      "css": ["assets/main.css"],
      "js": ["assets/main.js"]
    }
  ],

  "permissions": [
    "activeTab",
    "storage"
  ]
}